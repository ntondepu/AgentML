# Environment Configuration
APP_NAME=AutoML Distributed Platform
DEBUG=True
LOG_LEVEL=INFO
ENVIRONMENT=development

# Database Configuration
DATABASE_URL=postgresql://agentml:agentml_password@localhost:5432/agentml

# Redis Configuration
REDIS_URL=redis://localhost:6379

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_ARTIFACT_ROOT=s3://mlflow-artifacts/

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin

# AI/ML Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-3.5-turbo
HUGGINGFACE_TOKEN=your-huggingface-token-here
HUGGINGFACE_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Monitoring Configuration
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# Kubernetes Configuration
KUBERNETES_NAMESPACE=agentml
KUBERNETES_CONFIG_PATH=/path/to/kubeconfig

# Distributed Simulation Configuration
RAFT_NODE_COUNT=5
RAFT_HEARTBEAT_INTERVAL=0.5
RAFT_ELECTION_TIMEOUT=1.5

# Vector Database Configuration
VECTOR_DB_PATH=./data/vector_db
EMBEDDING_DIMENSION=384

# Data Configuration
DATA_DIR=./data
MODEL_DIR=./models

# Security Configuration
SECRET_KEY=your-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
